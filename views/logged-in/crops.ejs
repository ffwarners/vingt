    </nav><!--/nav-->

</header><!--/header-->


<section id="about-us">
    <div class="container">
        <div class="skill-wrap clearfix">
            <section id="recent-works">
                <div class="container">
                    <div class="center wow fadeInDown">
                        <h2>Onze wijnen</h2>
                        <p class="lead">Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod
                            tempor incididunt ut <br> et dolore magna aliqua. Ut enim ad minim veniam</p>
                    </div>

                    <ul class="portfolio-filter text-center">
                        <!--TODO: Controleer deze tekst-->
                        <li><a class="btn btn-default active" href="#" data-filter="*">Alle wijnen</a></li>
                        <li><a class="btn btn-default" href="#" data-filter=".under15">Prijs onder €15,-</a></li>
                        <li><a class="btn btn-default" href="#" data-filter=".droog">Droog</a></li>
                        <li><a class="btn btn-default" href="#" data-filter=".small">Kleiner dan 1L</a></li>
                    </ul><!--/#portfolio-filter-->

                    <div class="row">
                        <div class="portfolio-items">
                            <% wines.forEach(function(wine) { %>
                            <div class="portfolio-item apps col-xs-12 col-sm-4 col-md-3
                                 <% if (wine['aankoop prijs'] <= 15) { %> under15 <% } %>
                                 <% if (wine['inhoud'] <= 1.0) { %> small <% } %>
                                 <% if (wine['droog'] === 'true') { %> droog <% } %>">
                                <div class="recent-work-wrap">
                                    <img class="img-responsive" src="images/wines/Small/wine<%= wine.wine_id  %>.jpg"
                                         alt="">
                                    <div class="overlay">
                                        <div class="recent-work-inner">
                                            <h3><a href="/crop?id=<%= wine.wine_id %>"><%=  wine.naam  %></a></h3>
                                            <p>Geproduceerd door: <%=  wine.producent  %></p>
                                            <a class="preview"
                                               href="images/wines/wine<%=  wine.wine_id  %>.<%= wine.imageExt %>"
                                               rel="prettyPhoto"><i
                                                        class="fa fa-eye"></i> View</a>
                                        </div>
                                    </div>
                                </div>
                            </div><!--/.portfolio-item-->
                            <% });%>

                        </div>
                    </div>
                </div><!--/.container-->
            </section><!--/#recent-works-->
        </div>
    </div><!--/.container-->
</section><!--/about-us-->

