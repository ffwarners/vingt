<section id="blog" class="container" style="margin-top: 140px">
    <div class="center">
        <h2>Blogs</h2>
        <p class="lead">Controleer hier of er niks op de website komt wat er niet hoort. U kan niks wijzigen alleen uitschakelen en verwijderen</p>
    </div>

    <div class="blog">
        <div class="row">
            <div class="col-md-12">
                <% blog.forEach(function(reaction) { %>
                <div class="blog-item" id="div<%=reaction.id%>">
                    <div class="row" style="border: 2px solid red;">
                        <section>
                            <div class="checkboxThree"><input onchange="changeApproved('<%=reaction.id%>')" type="checkbox" <% if (reaction.approved == "true") { %> checked <% } %> value="1" id="checkboxThreeInput<%=reaction.id%>" name=""/><label for="checkboxThreeInput<%=reaction.id%>"></label></div>
                        </section>
                        <div onclick="deleteBlog('<%=reaction.id%>')" class="pull-right"><h1 style="color: black; margin-right: 40px;">x</h1></div>
                        <div class="col-xs-12 col-sm-10 blog-content">
                            <h4><strong>Aanmelder:</strong> <%= reaction.personName %></h4>
                            <h4><strong>Proeverij naam:</strong> <%= reaction.proeverijName %></h4>
                            <h4><strong>Proeverij datum:</strong> <%= reaction.proeverijDate %></h4>
                            <h4><strong>Reactie datum:</strong> <%= reaction.reactionDate %></h4>
                            <h4><strong>Rating:</strong> <%= reaction.rate %></h4>
                            <h4><strong>Reactie:</strong> <%= reaction.reaction %></h4>
                        </div>
                    </div>
                </div><!--/.blog-item-->
                <% }); %>
