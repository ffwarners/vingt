    <div class="blog">
        <div class="row">
            <div class="col-md-8">
                <div class="blog-item">
                    <div class="row">
                        <div class="col-xs-12 col-sm-2 text-center">
                            <div class="entry-meta">
                                <%var page = pagina *2%>
                                <% var date = blog[page-1].reactionDate.substring(8, 10); %>
                                <% var month = blog[page-1].reactionDate.substring(4, 7); %>
                                <% var name = blog[page-1].personName %>
                                <% name.length >= 10 ? name = name.substring(0, 10) + "..." : name; %>
                                <% var message = blog[page-1].reaction.substring(0, 10) + "..." %>
                                <span id="publish_date"><%= date + " " + month %></span>
                                <span><i class="fa fa-user"></i> <a href="#"><%= name %></a></span>
                                <span><i class="fa fa-comment"></i> <a href="blog-item.html#comments"><%= message %></a></span>
                                <span><i class="fa fa-star"></i><a href="#"><%= blog[page-1].rate %></a></span>
                            </div>
                        </div>

                        <div class="col-xs-12 col-sm-10 blog-content">
                            <a href="#"><img class="img-responsive img-blog" src="images/blog/blog1.jpg" width="100%"
                                             alt=""/></a>
                            <h2><a href="blog-item.html">Proeverij: <%= blog[page-1].proeverijName %></a></h2>
                            <h3><%= blog[page-1].reaction %></h3>
                            <a class="btn btn-primary readmore" href="/blog<%= blog[page-1].link %>">Meer lezen <i
                                        class="fa fa-angle-right"></i></a>
                        </div>
                    </div>
                </div><!--/.blog-item-->

                <% if(blog.length-1 >= page) { %>
                <div class="blog-item">
                    <div class="row">
                        <div class="col-sm-2 text-center">
                            <div class="entry-meta">
                                <% var current2 = page %>
                                <% var date2 = blog[current2].reactionDate.substring(8, 10); %>
                                <% var month2 = blog[current2].reactionDate.substring(4, 7); %>
                                <% var name2 = blog[current2].personName %>
                                <% name2.length >= 10 ? name2 = name2.substring(0, 10) + "..." : name2; %>
                                <% var message2 = blog[current2].reaction.substring(0, 10) + "..." %>
                                <span id="publish_date"><%= date2 + " " + month2 %></span>
                                <span><i class="fa fa-user"></i> <a href="#"><%= name2 %></a></span>
                                <span><i class="fa fa-comment"></i> <a href="blog-item.html#comments"><%= message2 %></a></span>
                                <span><i class="fa fa-star"></i><a href="#"><%= blog[current2].rate %></a></span>
                            </div>
                        </div>

                        <div class="col-xs-12 col-sm-10 blog-content">
                            <a href="#"><img class="img-responsive img-blog" src="images/blog/blog2.jpg" width="100%"
                                             alt=""/></a>
                            <h2><a href="blog-item.html">Proeverij: <%= blog[current2].proeverijName %></a></h2>
                            <h3><%= blog[current2].reaction %></h3>
                            <a class="btn btn-primary readmore" href="/blog<%= blog[current2].link %>">Meer lezen <i
                                        class="fa fa-angle-right"></i></a>
                        </div>
                    </div>
                </div><!--/.blog-item-->
                <% } %>
                <ul class="pagination pagination-lg">
                    <%if(page/2 >1) {%><li><a href="/blog?page=<%=page/2-1%>"><i class="fa fa-long-arrow-left"></i>Previous Page</a></li><%}%>
                    <% for(var i = 0; i < Math.ceil((blog.length-1) / 2); i++) { %>
                    <li
                            <% if(pagina - 1 == i) { %>
                            class="active"
                            <% } %>
                    ><a href="/blog?page=<%= i + 1 %>"><%= i + 1 %></a></li>
                    <% } %>
                    <%if(blog.length-1 > page) {%><li><a href="/blog?page=<%=page/2+ 1%>">Next Page<i class="fa fa-long-arrow-right"></i></a></li><%}%>
                </ul><!--/.pagination-->
